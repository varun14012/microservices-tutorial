<?xml version="1.0" encoding="UTF-8"?>
<!-- $Revision$  $Date$ -->
<schema xmlns="http://www.w3.org/2001/XMLSchema"
        xmlns:s="http://www.w3.org/2001/XMLSchema"
        targetNamespace="http://customer.ws.mcrewards.mastercard.com/"
        xmlns:tns="http://customer.ws.mcrewards.mastercard.com/"
        xmlns:common="http://common.ws.mcrewards.mastercard.com/"
        elementFormDefault="qualified">

  <s:import schemaLocation="CommonResponses.xsd" namespace="http://common.ws.mcrewards.mastercard.com/" />

  <s:element name="getCustomerInformation">
    <s:complexType>
      <s:sequence>
        <s:element minOccurs="0" name="language">
          <s:simpleType>
            <s:restriction base="string">
              <s:maxLength value="5"/>
            </s:restriction>
          </s:simpleType>
        </s:element>
        <s:element minOccurs="0" name="includeOnlyBasicCustomerInformation" type="common:BooleanYesNo" />
        <s:element minOccurs="0" name="includeOnlyDetailedCustomerInformation" type="common:BooleanYesNo" />
        <s:element minOccurs="0" name="includeOnlyMappedCustomerInformation" type="common:BooleanYesNo" />
        <s:element minOccurs="0" name="includeOnlyDetailedHouseholdInformation" type="common:BooleanYesNo" />
        <s:element minOccurs="0" maxOccurs="unbounded" name="customerInformationComponents" type="tns:CustomerInformationComponent" />
      </s:sequence>
    </s:complexType>
  </s:element>
  
  <s:simpleType name="CustomerInformationComponent">
    <s:restriction base="s:string">
      <s:enumeration value="cards" />
      <s:enumeration value="detailedCards" />
      <s:enumeration value="households" />
      <s:enumeration value="detailedHouseholds" />
      <s:enumeration value="pooling" />
      <s:enumeration value="maps" />
      <s:enumeration value="customerInformation" />
      <s:enumeration value="detailedCustomerInformation" />
    </s:restriction>
  </s:simpleType>

  <s:element name="CustomerInformation">
    <s:complexType>
      <s:sequence>
        <s:element minOccurs="0" name="icaCode">
          <s:annotation>
            <s:documentation>Parent level ICA code for members</s:documentation>
          </s:annotation>
          <s:simpleType>
            <s:restriction base="string">
              <s:maxLength value="10"/>
            </s:restriction>
          </s:simpleType>
        </s:element>
        <s:element minOccurs="0" name="referenceId">
          <s:annotation>
            <s:documentation>Customer Map id or the account map id </s:documentation>
          </s:annotation>
          <s:simpleType>
            <s:restriction base="string">
              <s:maxLength value="300"/>
            </s:restriction>
          </s:simpleType>
        </s:element>
        <s:element minOccurs="0" name="internetBankId" type="s:long" />
        <s:element minOccurs="0" maxOccurs="unbounded" name="cards" type="tns:Card" />
        <s:element minOccurs="0" maxOccurs="unbounded" name="households" type="tns:Household" />
        <s:element minOccurs="0" name="householdPoolingDetail" type="tns:HouseholdPoolingDetail"/>
        <s:element minOccurs="0" maxOccurs="unbounded" name="customerMaps" type="tns:CustomerMap" />
        <s:element minOccurs="0" name="customer" type="tns:Customer" />
      </s:sequence>
    </s:complexType>
  </s:element>

  <s:complexType name="Card">
    <s:sequence>
      <s:element minOccurs="0" name="bankAccountNumber" type="s:string" />
      <s:element minOccurs="0" name="lastFourDigitsOfCard" type="s:string" />
      <s:element minOccurs="0" name="accountId" type="s:long" />
      <s:element minOccurs="0" name="customerId" type="s:long" />
      <s:element minOccurs="0" name="householdRedeemerCode" type="s:string" />
      <s:element minOccurs="0" name="pointTotal" type="s:decimal" />
      <s:element minOccurs="0" name="accountStatusId" type="s:long" />
      <s:element minOccurs="0" name="accountStatusDesc" type="s:string" />
      <s:element minOccurs="0" name="bankProductName" type="s:string" />
      <s:element minOccurs="0" name="primaryAccountSw" type="s:boolean" />
      <s:element minOccurs="0" name="programId" type="s:long" />
      <s:element minOccurs="0" name="programIdentifier" type="s:string" />
      <s:element minOccurs="0" name="accruePointsSw" type="s:boolean" />
      <s:element minOccurs="0" name="accountOpenedDate" type="s:dateTime" />
      <s:element minOccurs="0" name="activeDate" type="s:dateTime" />
      <s:element minOccurs="0" name="autoEnrollSw" type="s:boolean" />
      <s:element minOccurs="0" name="dda" type="s:string" />
      <s:element minOccurs="0" name="enrollmentDate" type="s:dateTime" />
      <s:element minOccurs="0" name="enrollmentTypeId" type="s:long" />
      <s:element minOccurs="0" name="oldestLoyaltyTransactionDate" type="s:dateTime" />
      <s:element minOccurs="0" name="lifetimePointsAccrued" type="s:decimal" />
      <s:element minOccurs="0" name="lifetimePointsRedeemed" type="s:decimal" />
      <s:element minOccurs="0" name="lifetimeTransactionAmount" type="s:decimal" />
      <s:element minOccurs="0" name="lifetimeQuantityOfTransactions" type="s:decimal" />
      <s:element minOccurs="0" name="lostStolenSw" type="s:boolean" />
      <s:element minOccurs="0" name="previousBankAccountNumber" type="s:string" />
      <s:element minOccurs="0" name="proprietaryAccountStatusCode" type="s:string" />
      <s:element minOccurs="0" name="proprietaryAccountStatusDate" type="s:dateTime" />
      <s:element minOccurs="0" name="partnerId" type="s:string" />
      <s:element minOccurs="0" name="receiveStatementsSw" type="s:boolean" />
      <s:element minOccurs="0" name="reportCategoryText" type="s:string" />
      <s:element minOccurs="0" name="userDefined1" type="s:string" />
      <s:element minOccurs="0" name="userDefined2" type="s:string" />
      <s:element minOccurs="0" name="userDefined3" type="s:string" />
      <s:element minOccurs="0" name="userDefined4" type="s:string" />
      <s:element minOccurs="0" name="userDefined5" type="s:string" />
      <s:element minOccurs="0" name="userDefined6" type="s:string" />
      <s:element minOccurs="0" name="userDefined7" type="s:string" />
      <s:element minOccurs="0" name="userDefined8" type="s:string" />
      <s:element minOccurs="0" name="householdId" type="s:long" />
      <s:element minOccurs="0" name="customerIdentifier" type="s:boolean" />
      <s:element minOccurs="0" name="bankProductCode" type="s:string" />
      <s:element minOccurs="0" name="bankProductSegment" type="s:string" />
      <s:element minOccurs="0" name="coBrandId" type="s:string" />
      <s:element minOccurs="0" name="redeemerSw" type="s:boolean" />
      <s:element minOccurs="0" maxOccurs="unbounded" name="travelComponent" type="tns:TravelComponent"/>
      <s:element minOccurs="0" name="bankProductSegmentRank" type="s:long" />
      <s:element minOccurs="0" name="mastercardProductCode" type="s:string" />
      <s:element minOccurs="0" name="productTypeName" type="s:string" />
      <s:element minOccurs="0" name="productAliasName" type="s:string" />
      <s:element minOccurs="0" name="referenceId" type="s:string" />
    </s:sequence>
  </s:complexType>

  <s:complexType name="TravelComponent">
    <s:sequence>
      <s:element minOccurs="0" name="type" type="s:string"/>
      <s:element minOccurs="0" name="conversionFactor" type="s:decimal"/>
      <s:element minOccurs="0" name="minThreshold" type="s:decimal"/>
      <s:element minOccurs="0" name="pointIncrement" type="s:decimal"/>
    </s:sequence>
  </s:complexType>

  <s:complexType name="Household">
    <s:sequence>
      <s:element minOccurs="0" name="householdId" type="s:long" />
      <s:element minOccurs="0" name="householdRedeemerCode" type="s:string" />
      <s:element minOccurs="0" name="householdName" type="s:string" />
      <s:element minOccurs="0" name="householdPointBalance" type="s:decimal" />
      <s:element minOccurs="0" name="householdOnHoldPoints" type="s:decimal" />
    </s:sequence>
  </s:complexType>

  <s:complexType name="HouseholdPoolingDetail">
    <s:sequence>
      <s:element minOccurs="0" name="pointPoolingEnabled" type="s:boolean"/>
      <s:element minOccurs="0" name="totalPointBalance" type="s:decimal"/>
    </s:sequence>
  </s:complexType>

  <s:complexType name="CustomerMap">
    <s:sequence>
      <s:element minOccurs="0" name="mapTypeGroupCode" type="s:string" />
      <s:element minOccurs="0" name="mapTypeCode" type="s:string" />
      <s:element minOccurs="0" name="mapTypeName" type="s:string" />
      <s:element minOccurs="0" name="mapId" type="s:string" />
      <s:element minOccurs="0" name="mapStatus" type="s:string" />
      <s:element minOccurs="0" name="mapEnrollDate" type="s:dateTime" />
      <s:element minOccurs="0" name="mapExpirationDate" type="s:dateTime" />
      <s:element minOccurs="0" name="mapUserDefined1" type="s:string" />
      <s:element minOccurs="0" name="mapUserDefined2" type="s:string" />
      <s:element minOccurs="0" name="mapUserDefined3" type="s:string" />
      <s:element minOccurs="0" name="mapUserDefined4" type="s:string" />
      <s:element minOccurs="0" name="mapUserDefined5" type="s:string" />
    </s:sequence>
  </s:complexType>

  <s:complexType name="Customer">
    <s:sequence>
      <s:element minOccurs="0" name="customerID" type="s:string" />
      <s:element minOccurs="0" name="bankCustomerNumber" type="s:string" />
      <s:element minOccurs="0" name="primaryAccountHolderPrefix" type="s:string" />
      <s:element minOccurs="0" name="primaryAccountHolderFirstName" type="s:string" />
      <s:element minOccurs="0" name="primaryAccountHolderMiddleName" type="s:string" />
      <s:element minOccurs="0" name="primaryAccountHolderLastName" type="s:string" />
      <s:element minOccurs="0" name="primaryAccountHolderSuffix" type="s:string" />
      <s:element minOccurs="0" name="accountHolderAddressLine1" type="s:string" />
      <s:element minOccurs="0" name="accountHolderAddressLine2" type="s:string" />
      <s:element minOccurs="0" name="accountHolderAddressLine3" type="s:string" />
      <s:element minOccurs="0" name="accountHolderAddressLine4" type="s:string" />
      <s:element minOccurs="0" name="cityName" type="s:string" />
      <s:element minOccurs="0" name="state" type="s:string" />
      <s:element minOccurs="0" name="postalCode" type="s:string" />
      <s:element minOccurs="0" name="countryCode" type="s:string" />
      <s:element minOccurs="0" name="homePhoneNumber" type="s:string" />
      <s:element minOccurs="0" name="businessPhoneNumber" type="s:string" />
      <s:element minOccurs="0" name="faxPhoneNumber" type="s:string" />
      <s:element minOccurs="0" name="vipIndicator" type="s:string" />
      <s:element minOccurs="0" name="languageCode" type="s:string" />
      <s:element minOccurs="0" name="employeeSw" type="s:boolean" />
      <s:element minOccurs="0" name="ssnLastFour" type="s:string" />
      <s:element minOccurs="0" name="motherMaidenName" type="s:string" />
      <s:element minOccurs="0" name="genericIdentification" type="s:string" />
      <s:element minOccurs="0" name="genericIdentificationDescription" type="s:string" />
      <s:element minOccurs="0" name="birthDate" type="s:dateTime" />
      <s:element minOccurs="0" name="emailAddress" type="s:string" />
      <s:element minOccurs="0" name="acceptEmailMessageSw" type="s:boolean" />
      <s:element minOccurs="0" name="mobilePhoneNumber" type="s:string" />
      <s:element minOccurs="0" name="acceptSmsMessageSw" type="s:boolean" />
      <s:element minOccurs="0" name="gender" type="s:string" />
      <s:element minOccurs="0" name="userDefined1" type="s:string" />
      <s:element minOccurs="0" name="userDefined2" type="s:string" />
      <s:element minOccurs="0" name="userDefined3" type="s:string" />
      <s:element minOccurs="0" name="userDefined4" type="s:string" />
      <s:element minOccurs="0" name="proprietarySegmentCluster" type="s:string" />
      <s:element minOccurs="0" name="acceptPromotionalInformationSwitch" type="s:boolean" />
      <s:element minOccurs="0" name="acceptTrackingSwitch" type="s:boolean" />
      <s:element minOccurs="0" name="loginCount" type="s:long" />
      <s:element minOccurs="0" name="lifetimeRedemptionCount" type="s:long" />
    </s:sequence>
  </s:complexType>
  
  <s:element name="getCustomerAccounts">
    <s:complexType>
      <s:sequence>
        <s:element minOccurs="0" name="referenceId" type="tns:referenceId" />
        <s:element minOccurs="0" name="includeLoggedInCustomerAccountsOnly" type="s:boolean" default="true" />
      </s:sequence>
    </s:complexType>
  </s:element>

  <s:simpleType name="referenceId">
    <s:restriction base="s:string">
      <s:length value="30"/>
    </s:restriction>
  </s:simpleType>
  
  <s:element name="CustomerAccounts">
    <s:complexType>
      <s:sequence>
        <s:element minOccurs="0" maxOccurs="unbounded" name="customerAccount" type="tns:customerAccount" />
      </s:sequence>
    </s:complexType>
  </s:element>
  
  <s:complexType name="customerAccount">
    <s:sequence>
      <s:element name="belongsToLoggedInCustomer" type="s:boolean" />      
      <s:element name="accountId" type="s:long" />
      <s:element name="customerId" type="s:long" />
      <s:element name="householdId" type="s:long" />
      <s:element name="programId" type="s:long" />
      <s:element name="lastFourDigitsOfCard" type="s:string" />
      <s:element name="accountStatusId" type="s:long" />
      <s:element name="accountStatusDesc" type="s:string" />
      <s:element name="primaryAccountSw" type="s:boolean" />
      <s:element name="householdRedeemerCode" type="s:string" />
      <s:element name="bankProductName" type="s:string" />
      <s:element name="accruePointsSw" type="s:boolean" />
      <s:element name="autoEnrollSw" type="s:boolean" />
      <s:element name="enrollmentTypeId" type="s:long" />
      <s:element name="lostStolenSw" type="s:boolean" />
      <s:element name="receiveStatementsSw" type="s:boolean" />
      <s:element name="bankProductCode" type="s:string" />
      <s:element name="canRedeem" type="s:boolean" />
      <s:element name="productTypeName" type="s:string" />
      <s:element minOccurs="0" name="previousLastFourDigitsOfCard" type="s:string" />
      <s:element minOccurs="0" name="programIdentifier" type="s:string" />
      <s:element minOccurs="0" name="accountOpenedDate" type="s:dateTime" />
      <s:element minOccurs="0" name="activeDate" type="s:dateTime" />
      <s:element minOccurs="0" name="dda" type="s:string" />
      <s:element minOccurs="0" name="enrollmentDate" type="s:dateTime" />
      <s:element minOccurs="0" name="proprietaryAccountStatusCode" type="s:string" />
      <s:element minOccurs="0" name="proprietaryAccountStatusDate" type="s:dateTime" />
      <s:element minOccurs="0" name="partnerId" type="s:string" />
      <s:element minOccurs="0" name="reportCategoryText" type="s:string" />
      <s:element minOccurs="0" name="userDefined1" type="s:string" />
      <s:element minOccurs="0" name="userDefined2" type="s:string" />
      <s:element minOccurs="0" name="userDefined3" type="s:string" />
      <s:element minOccurs="0" name="userDefined4" type="s:string" />
      <s:element minOccurs="0" name="userDefined5" type="s:string" />
      <s:element minOccurs="0" name="userDefined6" type="s:string" />
      <s:element minOccurs="0" name="userDefined7" type="s:string" />
      <s:element minOccurs="0" name="userDefined8" type="s:string" />      
      <s:element minOccurs="0" name="bankProductSegment" type="s:string" />
      <s:element minOccurs="0" name="coBrandId" type="s:string" />
      <s:element minOccurs="0" name="bankProductSegmentRank" type="s:long" />
      <s:element minOccurs="0" name="mastercardProductCode" type="s:string" />
      <s:element minOccurs="0" name="productAliasName" type="s:string" />
      <s:element minOccurs="0" name="referenceId" type="s:string" />
      <s:element minOccurs="0" name="householdEligibilityToken" type="s:string" />
    </s:sequence>
  </s:complexType>

  <s:element name="getValidationQuestions">
    <s:complexType>
      <s:sequence>
        <s:element minOccurs="0" name="enterpriseSiteId" type="s:string" />
        <s:element minOccurs="1" name="bankAccountNumber" type="tns:encryptedField" />
        <s:element minOccurs="0" name="language" type="s:string" />
        <s:element minOccurs="0" name="customerId" type="s:long" />
      </s:sequence>
    </s:complexType>
  </s:element>
  
  <s:complexType name="encryptedField">
    <s:complexContent>
      <s:extension base="common:encryptedField" />
    </s:complexContent>
  </s:complexType>

  <s:element name="getValidationQuestionsResponse">
    <s:complexType>
      <s:sequence>
        <s:element minOccurs="0" maxOccurs="unbounded" name="questions" type="tns:VerificationQuestion" />
        <s:element minOccurs="0" name="programId" type="s:long" />
        <s:element minOccurs="0" name="confirmPartnerRegistrationId" type="s:string" />
        <s:element minOccurs="0" name="customerId" type="s:long" />
      </s:sequence>
    </s:complexType>
  </s:element>

  <s:complexType name="VerificationQuestion">
     <s:sequence>
      <s:element minOccurs="0" name="question" type="s:string" />
      <s:element minOccurs="0" name="code" type="s:string" />
    </s:sequence>
  </s:complexType>

  <s:element name="validateCustomer">
    <s:complexType>
      <s:sequence>
        <s:element minOccurs="0" name="enterpriseSiteId" type="s:string" />
        <s:element minOccurs="1" name="bankAccountNumber" type="tns:encryptedField" />
        <s:element minOccurs="0" maxOccurs="unbounded" name="validateAnswers" type="tns:ValidateAnswer" />
      </s:sequence>
    </s:complexType>
  </s:element>

  <s:complexType name="ValidateAnswer">
    <s:sequence>
      <s:element minOccurs="0" name="questionCode" type="s:string" />
      <s:element minOccurs="0" name="answer" type="tns:encryptedField" />
    </s:sequence>
  </s:complexType>

  <s:element name="validateCustomerResponse">
    <s:complexType>
      <s:sequence>
        <s:element minOccurs="0" name="confirmPartnerRegistrationId" type="s:string" />
      </s:sequence>
    </s:complexType>
  </s:element>

  <s:element name="getCustomerQualifications">
    <s:complexType>
      <s:sequence>
        <s:element name="qualificationLevel" type="tns:QualificationLevel" />
        <s:element minOccurs="0" name="qualificationStatus" type="tns:QualificationStatus" />
        <s:element minOccurs="0" name="fromDate" type="s:date" />
        <s:element minOccurs="0" name="toDate" type="s:date" />
      </s:sequence>
    </s:complexType>
  </s:element>

  <s:simpleType name="QualificationLevel">
    <s:restriction base="s:string">
      <s:enumeration value="Account" />
      <s:enumeration value="Customer" />
      <s:enumeration value="Household" />
    </s:restriction>
  </s:simpleType>
  
  <s:simpleType name="QualificationStatus">
    <s:restriction base="s:string">
      <s:enumeration value="Qualified" />
      <s:enumeration value="Processed" />
      <s:enumeration value="Deleted" />
      <s:enumeration value="Recurring" />
    </s:restriction>
  </s:simpleType>
  
  <s:element name="getCustomerQualificationsResponse">
    <s:complexType>
      <s:sequence>
        <s:element name="qualificationLevel" type="tns:QualificationLevel" />
        <s:element minOccurs="0" name="identifier" type="s:string" />
        <s:element minOccurs="0" maxOccurs="unbounded" name="qualification" type="tns:Qualification" />
      </s:sequence>
    </s:complexType>
  </s:element>
  
  <s:complexType name="Qualification">
    <s:sequence>
      <s:element minOccurs="0" name="qualificationId" type="s:string" />
      <s:element minOccurs="0" name="qualificationLevelId" type="s:string"/>
      <s:element minOccurs="0" name="externalQualificationId" type="s:string" />
      <s:element minOccurs="0" name="qualificationName" type="s:string" />
      <s:element minOccurs="0" name="qualificationDateTime" type="s:dateTime" />
      <s:element minOccurs="0" name="qualificationDesc" type="s:string" />
      <s:element minOccurs="0" name="qualificationStatus" type="tns:QualificationStatus" />
    </s:sequence>
  </s:complexType>
  
  <s:element name="getPointBalance">
    <s:complexType>
      <s:sequence>
        <s:element minOccurs="0" name="householdId" type="s:long" />
      </s:sequence>
    </s:complexType>
  </s:element>

  <s:element name="getPointBalanceResponse">
    <s:complexType>
      <s:sequence>
        <s:element minOccurs="0" maxOccurs="unbounded" name="pointbalances" type="tns:PointBalance" />
      </s:sequence>
    </s:complexType>
  </s:element>

  <s:complexType name="PointBalance">
    <s:sequence>
      <s:element minOccurs="0" name="householdId" type="s:long" />
      <s:element minOccurs="0" name="pointTotal" type="s:decimal" />
    </s:sequence>
  </s:complexType>
  
  <s:element name="getHouseholdPoints">
    <s:complexType>
      <s:sequence>
        <s:element name="householdId" type="s:long" />
      </s:sequence>
    </s:complexType>
  </s:element>

  <s:element name="getShippingAddress" type="tns:getShippingAddress" />
  
  <s:complexType name="getShippingAddress" />

  <s:element name="getShippingAddressResponse">
    <s:complexType>
      <s:sequence>
        <s:element minOccurs="0" maxOccurs="unbounded" name="shippingAddresses" type="tns:ShippingAddressExt" />
      </s:sequence>
    </s:complexType>
  </s:element>

  <s:complexType name="ShippingAddressExt">
    <s:complexContent mixed="false">
      <s:extension base="tns:ShippingAddress">
        <s:sequence>
          <s:element minOccurs="0" name="shippingAddressId" type="s:string" />
          <s:element minOccurs="0" name="addressLine4" type="s:string" />
          <s:element minOccurs="0" name="mobilePhoneNumber" type="s:string" />
        </s:sequence>
      </s:extension>
    </s:complexContent>
  </s:complexType>

  <s:complexType name="ShippingAddress">
    <s:sequence>
      <s:element minOccurs="0" name="shippingName" type="s:string" />
      <s:element minOccurs="0" name="companyName" type="s:string" />
      <s:element minOccurs="0" name="poBoxAddress" type="s:string" />
      <s:element minOccurs="0" name="poBoxAddressSw" type="s:boolean" />
      <s:element minOccurs="0" name="addressLine1" type="s:string" />
      <s:element minOccurs="0" name="addressLine2" type="s:string" />
      <s:element minOccurs="0" name="addressLine3" type="s:string" />
